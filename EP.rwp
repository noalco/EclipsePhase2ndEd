    <LUACode>function onClickRelease(button, x, y)
	if getValue()==0 then
		local sPronoun="";
		if DB.getValue(window.getDatabaseNode(), "gender", ""):lower():match("male") then sPronoun="his";
		elseif DB.getValue(window.getDatabaseNode(), "gender", ""):lower():match("female") then sPronoun="her";
		else sPronoun="its";
		end
		window.InsightSpent.setValue(0);
		window.MoxieSpent.setValue(0);
		window.VigorSpent.setValue(0);
		window.FlexSpent.setValue(0);
--		Tools.ChatMenssage(DB.getValue(window.getDatabaseNode(), "name", "") .. " performs a Long Recharge.", "msgfont", "portrait_gm_token");
		local rMessage = ChatManager.createBaseMessage();
		rMessage.text=DB.getValue(window.getDatabaseNode(), "name", "") .. " performs a Long Recharge and restores all "..sPronoun.." pool points.";
		Comm.deliverChatMessage(rMessage);
	end
end
</LUACode>
