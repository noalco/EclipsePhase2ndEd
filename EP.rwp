    <LUACode>local nPrev=0;

function onValueChanged()
	if getValue()&gt;nPrev then
		local rRoll = { sType = "ShortRecharge", sDesc = "", aDice = {"d6"}, nMod = 0 };	
		ActionsManager.performAction(nil,nil,rRoll);
	end
	nPrev=getValue();
end

function onInit()
	nPrev=getValue();
	ActionsManager.registerResultHandler("ShortRecharge", onRoll);
end

function onRoll(rSource, rTarget, rRoll)
	local rMessage = ActionsManager.createActionMessage(rSource, rRoll);
	rMessage.text=DB.getValue(window.getDatabaseNode(), "name", "") .. " performs a Short Recharge and restores "..ActionsManager.total(rRoll).." pool points.";
	Comm.deliverChatMessage(rMessage);
end
</LUACode>
