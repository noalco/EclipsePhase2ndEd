    <LUACode>function onInit()
	local nodeChar=getDatabaseNode();
	nodeChar=nodeChar.getPath();
	DB.addHandler(nodeChar..".Muse.Aptitudes.Scores", "onChildUpdate", onMeshAptitudeUpdate);
	onMeshAptitudeUpdate();
end

function onMeshAptitudeUpdate()
	local sSkillName;
	local sApt;
	local nodeChar=getDatabaseNode();
	local nMult
--	local tApt={["COG"]="Cognition", ["INT"]="Intuition", ["REF"]="Reflexes", ["SAV"]="Savvy", ["SOM"]="Somatics", ["WIL"]="Willpower"}
	for _,nodeSkill in pairs(SkillList.getWindows()) do
		sSkillName=nodeSkill.Name.getValue();
		if sSkillName=="Know (INT)" then nodeSkill.AptitudeBonus.setValue(Intuition.getValue());
		elseif sSkillName=="Know (COG)" then nodeSkill.AptitudeBonus.setValue(Cognition.getValue());
		elseif sSkillName~="" then
			sApt=DataCommon.ActiveSkills[sSkillName].sApt;
			nMult=DataCommon.ActiveSkills[sSkillName].nAptMult;
			if sApt~="" then
				nodeSkill.AptitudeBonus.setValue(DB.getValue(nodeChar, ".Muse"..DataCommon.Aptitudes[sApt].sPath)*nMult);
			else
				nodeSkill.AptitudeBonus.setValue(0);
			end
		end
	end
end
</LUACode>
