function Update()
	local nPhAP=0;
	if window.Type.getValue()=="Physical" or window.Type.getValue()=="Combat" then
		nPhAP=window.PhAP.getValue();
	end
	if window.name.getValue()=="Fray/2 (vs. ranged attacks)" then
		setValue(Tools.RoundNumber((window.AptitudeBonus.getValue()*window.AptMult.getValue()+window.Background.getValue()+window.Career.getValue()+window.Interest.getValue()+window.CustomizationPoints.getValue()+nPhAP)/2));
	else
		setValue(window.AptitudeBonus.getValue()*window.AptMult.getValue()+window.Background.getValue()+window.Career.getValue()+window.Interest.getValue()+window.CustomizationPoints.getValue()+nPhAP);
	end
end

