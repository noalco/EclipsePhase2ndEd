    <LUACode>function onValueChanged()
	local sApt=getStringValue();
	if sApt and sApt~="" and DataCommon.Aptitudes[sApt] then
--		Aptitudes = {["COG"] = {sPath=".Aptitudes.Checks.Cognition", sPool="Insight"},
		local tAptitude=DataCommon.Aptitudes[sApt];
		DB.setValue(window.getDatabaseNode(), "pool", "string", tAptitude.sPool);
		local sNodeCharPath=Tools.getCharNodePath(window.getDatabaseNode())
		window.AptitudeBonus.setValue(DB.getValue(sNodeCharPath..tAptitude.sPath, 0));
	else
		DB.setValue(window.getDatabaseNode(), "pool", "string", "");
		window.AptitudeBonus.setValue(0);		
	end
end
</LUACode>
