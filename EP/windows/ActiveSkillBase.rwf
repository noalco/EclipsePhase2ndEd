    <LUACode>function updateWindow()
--	ActiveSkills = {["Athletics"] = {sApt="SOM", nAptMult=1, sType="Physical", bHasField=false},
	local sSkillName=name.getValue();
	if sSkillName=="Melee" or sSkillName=="Guns" then
		local sNodeChar=Tools.getCharNodePath(getDatabaseNode());
		local sWindowPath=getDatabaseNode().getPath();
		if sSkillName=="Melee" and DB.getValue(sNodeChar..".Paths.MeleeSkillPath")~=sWindowPath then
			DB.setValue(sNodeChar..".Paths.MeleeSkillPath", "string", sWindowPath);
		elseif sSkillName=="Guns" and DB.getValue(sNodeChar..".Paths.GunsFraySkillPath")~=sWindowPath then
			DB.setValue(sNodeChar..".Paths.GunsFraySkillPath", "string", sWindowPath);
		end
	end
	local tSkill=DataCommon.ActiveSkills[sSkillName];
	if tSkill then
--		Aptitudes = {["COG"] = {sPath=".Aptitudes.Checks.Cognition", sPool="Insight"},
		local tAptitude=DataCommon.Aptitudes[tSkill.sApt];
		if tAptitude then
			aptitude.setReadOnly(true);
			Type.setReadOnly(true);
			AptitudeBonus.setValue(DB.getValue(tAptitude.sPath, 0));
			AptMult.setReadOnly(true);
			if AptMult.getValue()==1 then
				AptMult.setVisible(false);
				LabelX.setVisible(false);
			end
		else
			aptitude.setReadOnly(false);
			Type.setReadOnly(false);
			AptMult.setReadOnly(false);
		end
	else
		aptitude.setReadOnly(false);
		Type.setReadOnly(false);
		AptMult.setReadOnly(false);
	end
	local sApt=aptitude.getStringValue();
	if sApt~="" then
		local sNodeCharPath=Tools.getCharNodePath(getDatabaseNode())
		AptitudeBonus.setValue(DB.getValue(sNodeCharPath..DataCommon.Aptitudes[sApt].sPath, 0));
	end
end

function FindSkillWithField(sSkillType, sSkill, sField)
	sField=sField:lower();
	for _,nodeSkill in pairs(DB.getChildren(Tools.getCharNodePath(getDatabaseNode()).."."..sSkillType..".SkillList")) do
		if DB.getValue(nodeSkill, "Name", "")==sSkill and DB.getValue(nodeSkill, "Field", ""):lower()==sField then
			return DB.getValue(nodeSkill, "Score", 0);
		end
	end
	return 0;
end

function UpdateBonus()
	local sSkillName=name.getValue();
	Background.setValue(FindSkillWithField("Background", sSkillName, Field.getValue()));
	Career.setValue(FindSkillWithField("Career", sSkillName, Field.getValue()));
	Interest.setValue(FindSkillWithField("Interest", sSkillName, Field.getValue()));
end

function GetRollMessage()
	local sMessage=name.getValue();
	if Field.getValue()~="" then
		sMessage=sMessage..": "..Field.getValue();
	end
	return sMessage;
end

function GetRollSpecializationMessage()
	local sMessage=GetRollMessage();
	if Specialization.getValue()==1 and SpecializationName.getValue()~="" then
		sMessage=sMessage.." - "..SpecializationName.getValue();
	end
	return sMessage;
end




local sFraySkillPath="";

function UpdateSpec()
	local bIsFray2=name.getValue()=="Fray/2 (vs. ranged attacks)";
	Specialization.setEnabled(not bIsFray2);
	SpecializationName.setEnabled(not bIsFray2);
	if bIsFray2 then
		if DB.getValue(sFraySkillPath..".Specialization", 0)==1 then
			SpecializationName.setValue(DB.getValue(sFraySkillPath..".SpecializationName", ""));
			Specialization.setValue(1);
		else
			Specialization.setValue(0);
		end
	end
end

function UpdateFraySkillPath()
	if sFraySkillPath~="" then
		DB.removeHandler(sFraySkillPath, "onChildUpdate", UpdateSpec);
	end
	sFraySkillPath=DB.getValue(Tools.getCharNodePath(getDatabaseNode())..".Paths.FraySkillPath", "");
	if sFraySkillPath~="" then
		DB.addHandler(sFraySkillPath, "onChildUpdate", UpdateSpec);
	end
	UpdateSpec();
end

function onInit()
--	local bIsFray2=name.getValue()=="Fray/2 (vs. ranged attacks)";
	if name.getValue()=="Fray/2 (vs. ranged attacks)" then
		DB.addHandler(Tools.getCharNodePath(getDatabaseNode())..".Paths.FraySkillPath", "onUpdate", UpdateFraySkillPath);
		UpdateFraySkillPath();
	else
		UpdateSpec();
	end
end

function onClose()
--	local bIsFray2=name.getValue()=="Fray/2 (vs. ranged attacks)";
	if name.getValue()=="Fray/2 (vs. ranged attacks)" then
		DB.removeHandler(Tools.getCharNodePath(getDatabaseNode())..".Paths.FraySkillPath", "onUpdate", UpdateFraySkillPath);
		if sFraySkillPath~="" then
			DB.removeHandler(sFraySkillPath, "onChildUpdate", UpdateSpec);
		end
	end
end</LUACode>
