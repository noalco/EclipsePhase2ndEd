    <LUACode>function GetRollMessage()
	local sMessage="Know: "..name.getValue();
	if isDefaulting()=="true" then
		sMessage="[DEFAULTING] "..sMessage;
	end
	return sMessage;
end

function isDefaulting()
	if Background.getValue()==0 and Career.getValue()==0 and Interest.getValue()==0 and Faction.getValue()==0 and CustomizationPoints.getValue()==0 then
		return "true";
	else
		return "false";
	end
end

function GetRollSpecializationMessage()
	local sMessage=GetRollMessage();
	if Specialization.getValue()==1 and SpecializationName.getValue()~="" then
		sMessage=sMessage.." - "..SpecializationName.getValue();
	end
	return sMessage;
end

function updateWindow()
	local sApt=aptitude.getStringValue();
	if sApt~="" then
		local sNodeCharPath=Tools.getCharNodePath(getDatabaseNode())
		AptitudeBonus.setValue(DB.getValue(sNodeCharPath..DataCommon.Aptitudes[sApt].sPath, 0));
	end
end</LUACode>
