    <LUACode>function CopyLink(dragdata, sNodeType, Node)
	local sClass,sRecord = dragdata.getShortcutData();
	if sClass==sNodeType.."SidebarWindow" or sClass==sNodeType.."Window" then
		if sRecord~=DB.getPath(Node) then
			DB.deleteChildren(DB.getChild(Node, "SkillList"));
			DB.copyNode(sRecord, Node);
		end
	end
end
</LUACode>
