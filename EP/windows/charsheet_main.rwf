function onValueChanged()
	local sClass, sRecord=DB.getValue(window.getDatabaseNode(), "Morph.link");
	if sRecord and sRecord~="" then
		window.InnerPoolsValuesSubwindow.subwindow.Insight.setLink(DB.createChild(sRecord, "Insight", "number"), true);
		window.InnerPoolsValuesSubwindow.subwindow.Moxie.setLink(DB.createChild(sRecord, "Moxie", "number"), true);
		window.InnerPoolsValuesSubwindow.subwindow.Vigor.setLink(DB.createChild(sRecord, "Vigor", "number"), true);
		window.InnerPoolsValuesSubwindow.subwindow.Flex.setLink(DB.createChild(sRecord, "Flex", "number"), true);
		window.Durability.setLink(DB.createChild(sRecord, "Durability", "number"), true);
		window.MorphName.setLink(DB.createChild(sRecord, "name", "string"), true);
		window.MorphType.setLink(DB.createChild(sRecord, "MorphType", "string"), true);
		window.MovementRate.setLink(DB.createChild(sRecord, "MovementRate", "string"), true);
		window.MorphTraitsDetailsLink.setValue("MorphTraitsDetailsWindow", sRecord);
		window.MorphWareDetailsLink.setValue("MorphWareDetailsWindow", sRecord);
		
		
	else
		setValue(nil, nil);
		window.MorphTraitsDetailsLink.setValue(nil, nil);
		window.MorphWareDetailsLink.setValue(nil, nil);
	end
end

