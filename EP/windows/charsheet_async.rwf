function onInit()
	DB.addHandler(DB.getPath(window.getDatabaseNode(), "Ego.TraitList.*.name"), "onUpdate", UpdateValue);
	DB.addHandler(DB.getPath(window.getDatabaseNode(), "Ego.TraitList.*.TraitLevel"), "onUpdate", UpdateValue);
	UpdateValue();
end

function UpdateValue()
	for _,n in pairs(DB.getChildren(window.getDatabaseNode(), "Ego.TraitList")) do
		if DB.getValue(n, "name", ""):lower()=="psi" then
			setValue(10*DB.getValue(n, "TraitLevel", 1));
			return;
		end
	end
	setValue(0);
end

function onDrop(x, y, dragdata)
	window.LinkSubStrain.onDrop(x, y, dragdata);
end
