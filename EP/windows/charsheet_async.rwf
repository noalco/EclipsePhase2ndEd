    <LUACode>nInfluenceEventMod=0;

function getRollString()
	local sMod=""
	if nInfluenceEventMod&gt;0 then
		sMod="+"..nInfluenceEventMod;
		nInfluenceEventMod=0;
	end
	return "1d6"..sMod;
end

function onInit()
	nInfluenceEventMod=0;
end

function InfluenceEventResult(rSource, rTarget, rRoll)
	Comm.deliverChatMessage(ActionsManager.createActionMessage(rSource, rRoll));
	local rMessage = ChatManager.createBaseMessage();
	rMessage.text="Influence Event Result: ";
	if rRoll.nTotal==1 then
		rMessage.text=rMessage.text..window.InfluenceEvent1.getValue();
	elseif rRoll.nTotal==2 then
		rMessage.text=rMessage.text..window.InfluenceEvent2.getValue();
	elseif rRoll.nTotal==3 then
		rMessage.text=rMessage.text..window.InfluenceEvent3.getValue();
	elseif rRoll.nTotal==4 then
		rMessage.text=rMessage.text..window.InfluenceEvent4.getValue();
	elseif rRoll.nTotal==5 then
		rMessage.text=rMessage.text..window.InfluenceEvent5.getValue();
	else
		rMessage.text=rMessage.text..window.InfluenceEvent6.getValue();
	end
	Comm.deliverChatMessage(rMessage);
end
