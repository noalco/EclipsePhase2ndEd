    <LUACode>function onInit()
	local sCharPath=Tools.getCharNodePath(window.getDatabaseNode());
	if sCharPath~="" then
		DB.addHandler(DB.getPath(sCharPath..".Aptitudes.Scores.Willpower"), "onUpdate", UpdateValue);
	end
	UpdateValue();
end

function onClose()
	local sCharPath=Tools.getCharNodePath(window.getDatabaseNode());
	if sCharPath~="" then
		DB.removeHandler(DB.getPath(sCharPath..".Aptitudes.Scores.Willpower"), "onUpdate", UpdateValue);
	end
end

function UpdateValue()
	local sCharPath=Tools.getCharNodePath(window.getDatabaseNode());
	if sCharPath~="" then
		setValue("Duration ("..Tools.RoundNumber(DB.getValue(sCharPath..".Aptitudes.Scores.Willpower", 5)/5)..")");
		setTooltipText("(x) = WIL รท 5");
	end
end</LUACode>
