    <LUACode>function onValueChanged()
	if getValue() == "Psi-gamma" then
		window.Action.setReadOnly(false);
		window.Duration.setReadOnly(false);
		window.InfectionMod.setReadOnly(false);
	else
		window.Action.setReadOnly(true);
		window.Duration.setReadOnly(true);
		window.InfectionMod.setReadOnly(true);
		window.Action.setStringValue("Automatic");
		window.Duration.setStringValue("Constant");
		window.InfectionMod.setValue(0);
	end
end

function onInit()
	super.onInit();
	onValueChanged();
end
