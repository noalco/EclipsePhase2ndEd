    <LUACode>sModMessage="";

function getPsiTargetValue()
	sModMessage="";
	local nPsiTarget=0;
	local windowNode=window.windowlist.window;
	for _,n in pairs(DB.getChildren(windowNode.getDatabaseNode(), "Skills.ActiveSkillsList")) do
		if DB.getValue(n, "name", "")=="Psi" then
			nPsiTarget=DB.getValue(n, "Total", 0);
			break;
		end
	end
	local sRange=windowNode.SleightRange.getValue();
	if sRange=="Point-Blank" then
		nPsiTarget=nPsiTarget+10;
		sModMessage=" [Point-Blank: +10]";
	elseif sRange=="Close" then
		if windowNode.ExtraDist.getValue()&gt;0 then
			nPsiTarget=nPsiTarget-10*math.floor(windowNode.ExtraDist.getValue()/2);
			sModMessage=" [Close: -"..10*math.floor(windowNode.ExtraDist.getValue()/2).."]";
		end
	else
		nPsiTarget=nPsiTarget+20;
		sModMessage=" [Touch: +20]";
	end
	
	local sTargetType=windowNode.TargetType.getValue();
	if sTargetType=="Cyberbrain" then
		nPsiTarget=nPsiTarget-30;
		sModMessage=sModMessage.."[Cyberbrain target: -30]";	
	elseif sTargetType=="Partially sapient/uplifted animal" then
		nPsiTarget=nPsiTarget-20;
		sModMessage=sModMessage.."[Partially sapient/uplifted animal target: -20]";
	elseif sTargetType=="Non-sapient animal" then
		nPsiTarget=nPsiTarget-30;
		sModMessage=sModMessage.."[Non-sapient animal target: -30]";
	elseif sTargetType=="Factor/Alien" then
		nPsiTarget=nPsiTarget-20;
		sModMessage=sModMessage.."[Factor/Alien target: -20]";
	end
	
	return nPsiTarget;
end

function CastSleightHandler(rSource, rTarget, rRoll)
	RollHandlers.CheckHandler(rSource, rTarget, rRoll);
	window.windowlist.window.InfectionRatingIncrease.setValue(window.windowlist.window.InfectionRatingIncrease.getValue()+window.InfectionMod.getValue());
end
</LUACode>
