    <LUACode>function MostrarEtiquetas()
	local bPositive=Positive.getValue()==1;
	local bIsEgo=IsEgo.getValue()==1;
	local bIsMorph=IsMorph.getValue()==1;
	
	LabelCPCost.setVisible(false);
	LabelMPCost.setVisible(false);
	LabelCPMPCost.setVisible(false);
	LabelCPBonus.setVisible(false);
	LabelMPBonus.setVisible(false);
	LabelCPMPBonus.setVisible(false);
	CostBonus.setVisible(bIsEgo or bIsMorph);
	
	if bPositive then
		if bIsEgo and bIsMorph then LabelCPMPCost.setVisible(true);
		elseif bIsEgo then LabelCPCost.setVisible(true);
		elseif bIsMorph then LabelMPCost.setVisible(true);
		end
	else
		if bIsEgo and bIsMorph then LabelCPMPBonus.setVisible(true);
		elseif bIsEgo then LabelCPBonus.setVisible(true);
		elseif bIsMorph then LabelMPBonus.setVisible(true);
		end	
	end
end

function onInit()
	MostrarEtiquetas();
end
</LUACode>
