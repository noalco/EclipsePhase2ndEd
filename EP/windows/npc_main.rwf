    <LUACode>function onInit()
	super.onInit();
	DB.addHandler(DB.getPath(window.getDatabaseNode(), "Skills"), "onUpdate", UpdateValue);
	UpdateValue();
end

function onClose()
	DB.removeHandler(DB.getPath(window.getDatabaseNode(), "Skills"), "onUpdate", UpdateValue);
end

function UpdateValue()
	local sSkills=DB.getValue(window.getDatabaseNode(), "Skills", "");
	local nSpecializationName, nSpecializationValue=sSkills:lower():match("melee%s+%d+%s*%((.*)%s+(%d+)%)");
	if nSpecializationName and nSpecializationValue then
		setValue(nSpecializationValue);
		window.HasMeleeSpecialization.setValue(1);
		window.MeleeSpecializationName.setValue(nSpecializationName);
		setVisible(true);
		window.LabelMeleeSpecialization.setVisible(true);
	else
		window.HasMeleeSpecialization.setValue(0);
		setVisible(false);
		window.LabelMeleeSpecialization.setVisible(false);
	end
end

