	setValue(Tools.RoundNumber((window.Reflexes.getValue()+window.Intuition.getValue())/5))
end

function onInit()
	Update();
	super.onInit();
end

function getRoll()	
	local nWoundMod=-1*DB.getValue(window.getDatabaseNode(), "Health.Physical.WoundsTaken", 0);
	sWoundMod="";
	if nWoundMod~=0 then
		sWoundMod="[WOUNDS: "..nWoundMod.."]";
	end

	local nMod=getValue()+nWoundMod;
	
	if nMod==0 then
		return "1d6";
	elseif nMod&gt;0 then
		return "1d6+"..nMod;
	else
		return "1d6"..nMod;
	end
end</LUACode>
