    <LUACode>function Update()
	local sFM="";
	local sConnector="";
	if window.FiringModeSubwindow.subwindow.SingleShot.getValue()==1 then
		sFM="SS";
		sConnector="/";
	end
	if window.FiringModeSubwindow.subwindow.SemiAutomatic.getValue()==1 then
		sFM=sFM..sConnector.."SA";
		sConnector="/";
	end
	if window.FiringModeSubwindow.subwindow.BurstFire.getValue()==1 then
		sFM=sFM..sConnector.."BF";
		sConnector="/";
	end
	if window.FiringModeSubwindow.subwindow.FullAutomatic.getValue()==1 then
		sFM=sFM..sConnector.."FA";
	end
	setValue(sFM);
end</LUACode>
