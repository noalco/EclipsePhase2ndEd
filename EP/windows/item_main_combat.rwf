    <LUACode>function update(bReadOnly)
	local sType=window.WeaponType.getValue()
	local bReadOnlyState;
	if sType=="melee" then
		bReadOnlyState=false;
	elseif sType=="beam" or sType=="spray" then
		bReadOnlyState=true;
	elseif sType=="thrown" then
		bReadOnlyState=false;
	elseif sType=="system" then
		bReadOnlyState=true;
	else
		bReadOnlyState=false;
	end
	
	setReadOnly(bReadOnly or bReadOnlyState);
	local bVisible=not (bReadOnly and getValue()=="") and (window.Type.isMeleeWeapon() or window.Type.isRangedWeapon());
	setVisible(bVisible);
	window.LabelAttackSkill.setVisible(bVisible);
end

function UpdateAttackSkill()
	local sType=window.WeaponType.getValue();	
	if sType=="melee" then
		setValue("Melee");
		setReadOnly(false);
	elseif sType=="beam" or sType=="spray" then
		setValue("Guns");
		setReadOnly(true);
	elseif sType=="thrown" then
		setValue("Athletics");
		setReadOnly(false);
	elseif sType=="system" then
		setValue("Interface");
		setReadOnly(true);
	else
		setValue("");
		setReadOnly(false);
	end
end

-- 	Attack skills:
--		Melee and Improvised Weapons
--			Melee (*)
--			Exotic skill
--		Ranged weapons
--			Beam and Spray Weapons
--				Guns
--			Thrown Weapons
--				Athletics (*)
--				Exotic skill
--			Weapon Systems
--				Interface
--	(*) standard skill</LUACode>
