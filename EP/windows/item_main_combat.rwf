    <LUACode>function update(bReadOnly)
	setReadOnly(bReadOnly);
	window.DamagePlus.setReadOnly(bReadOnly);
	window.DamageAverage.setReadOnly(bReadOnly);
	
	local bVisible=not (bReadOnly and #getDice()==0) and window.Type.isWeapon();
	local bVisiblePlus=not (bReadOnly and window.DamagePlus.getValue()==0) and window.Type.isWeapon();
	local bVisibleAverage=not (bReadOnly and window.DamageAverage.getValue()==0) and window.Type.isWeapon();
	
	setVisible(bVisible);
	window.DamagePlus.setVisible(bVisiblePlus);
	window.DamageAverage.setVisible(bVisibleAverage);

	window.LabelDamage.setVisible(bVisible or bVisiblePlus or bVisibleAverage);
	window.LabelDamagePlus.setVisible(bVisible and bVisiblePlus);
	window.LabelAverage.setVisible(bVisibleAverage);
end
</LUACode>
