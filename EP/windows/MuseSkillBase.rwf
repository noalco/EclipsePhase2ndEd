    <LUACode>function GetRollMessage()
	local sMessage=Name.getValue();
	if sMessage:match("Know") then sMessage="Know"; end
	if sMessage~="" and (sMessage=="Know" or DataCommon.ActiveSkills[sMessage].bHasField) then
		sMessage=sMessage..": "..Field.getValue();
	end
	return sMessage;
end
