    <LUACode>function onValueChanged()
	local bVisible=getStringValue()~="" and (getStringValue():match("Know") or DataCommon.ActiveSkills[getStringValue()].bHasField);
	window.Field.setVisible(bVisible);
	local nodeChar=Tools.getCharNodePath(getDatabaseNode());
	local sSkillName=getValue();
	if sSkillName=="Know (INT)" then
		window.AptitudeBonus.setValue(DB.getValue(nodeChar..".Muse.Aptitudes.Scores.Intuition"));
	elseif sSkillName=="Know (COG)" then
		window.AptitudeBonus.setValue(DB.getValue(nodeChar..".Muse.Aptitudes.Scores.Cognition"));
	elseif sSkillName~="" then
		local sApt=DataCommon.ActiveSkills[sSkillName].sApt;
		local nMult=DataCommon.ActiveSkills[sSkillName].nAptMult;
		if sApt~="" then
			window.AptitudeBonus.setValue(DB.getValue(nodeChar..".Muse"..DataCommon.Aptitudes[sApt].sPath)*nMult);
		else
			window.AptitudeBonus.setValue(0);
		end
	end
end

function onInit()
	super.onInit();
	onValueChanged();
end
