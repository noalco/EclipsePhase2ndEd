    <LUACode>function UpdateValue()
	setValue(window.InfectionRatingBase.getValue()+window.InfectionRatingIncrease.getValue())
end

function onValueChanged()
	if getValue()&lt;33 then
		window.Label33.setFont("small_label_grey");
		window.Label66.setFont("small_label_grey");
	elseif getValue()&lt;66 then
		window.Label33.setFont("small_label");
		window.Label66.setFont("small_label_grey");
	else
		window.Label33.setFont("small_label_grey");
		window.Label66.setFont("small_label");
	end
end

function onInit()
	super.onInit();
	onValueChanged();
end

function CheckInfectorTextHandler(rSource, rTarget, rRoll)
	local sResult=RollHandlers.CheckHandler(rSource, rTarget, rRoll);
	local rMessage = ChatManager.createBaseMessage();
	local bRollInfluenceEvent=true;
	if sResult=="SUCCESS" then
		rMessage.text="You suffer an influence effect.";
	elseif sResult=="ONE SUPERIOR SUCCESS" then
		window.ButtonRollInfluenceEvent.nInfluenceEventMod=1;
		rMessage.text="You suffer an influence effect. A +1 modifier is applied to your influence effect roll";
	elseif sResult=="TWO SUPERIOR SUCCESSES" then
		window.ButtonRollInfluenceEvent.nInfluenceEventMod=2;
		rMessage.text="You suffer an influence effect. A +2 modifier is applied to your influence effect roll";
	elseif sResult=="CRITICAL SUCCESS" then
		rMessage.text="You suffer an influence effect and the GM also picks between Checkout Time and Interference";
	elseif sResult=="CRITICAL FAILURE" then
		rMessage.text="You are immune to influence effects until your next recharge.";
	else -- FAILURE
		rMessage.text="You pull off the sleight without any negative effects.";
	end
	Comm.deliverChatMessage(rMessage);
end
