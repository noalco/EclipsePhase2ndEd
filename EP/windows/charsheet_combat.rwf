    <LUACode>function onInit()
	DB.addHandler(DB.getPath(getDatabaseNode(), "inventorylist.*.Type"), "onUpdate", UpdateCombatList);
	DB.addHandler(DB.getPath(getDatabaseNode(), "inventorylist"), "onChildAdded", UpdateCombatList);
	DB.addHandler(DB.getPath(getDatabaseNode(), "inventorylist"), "onChildDeleted", UpdateCombatList);
	
	attacks.OldAddEntry=attacks.addEntry;
	attacks.addEntry=NewAddEntry
	
	UpdateCombatList();
end

function onClose()
	DB.removeHandler(DB.getPath(getDatabaseNode(), "inventorylist.*.Type"), "onUpdate", UpdateCombatList);
	DB.removeHandler(DB.getPath(getDatabaseNode(), "inventorylist"), "onChildAdded", UpdateCombatList);
	DB.removeHandler(DB.getPath(getDatabaseNode(), "inventorylist"), "onChildDeleted", UpdateCombatList);
end

function UpdateCombatList()
	local sType;
	for _,w in pairs(attacks.getWindows()) do
		sType=DB.getValue(w.getDatabaseNode(), "Type", "");
		w.Attack.setVisible((sType=="Melee Weapon" or sType=="Ranged Weapon" or sType=="Micromissile/Minigrenade") and (Mode.getValue()=="Preparation" or Mode.getValue()=="Combat" and w.Attack.subwindow.carried.getValue()==2));
		w.Attack.subwindow.carried.UpdateVisibility();
	end
end

function NewAddEntry(bFocus)
	local w=attacks.OldAddEntry(bFocus);
	DB.setValue(w.getDatabaseNode(), "Type", "string", "Melee Weapon");
	DB.setValue(w.getDatabaseNode(), "Typevalue", "string", "Melee Weapon");
	DB.setValue(w.getDatabaseNode(), "count", "number", 1);
	return w;
end</LUACode>
