    <LUACode>function Update()
	sSkillPath=DB.getValue(Tools.getCharNodePath(getDatabaseNode())..".Paths.FraySkillPath", "");
	if sSkillPath~="" then setValue(DB.getValue(sSkillPath..".Total", 0)); end;
end

function onInit()
	sSkillPath=DB.getValue(Tools.getCharNodePath(getDatabaseNode())..".Paths.FraySkillPath", "");
	if sSkillPath~="" then
		DB.addHandler(sSkillPath, "onChildUpdate", Update);
		Update();
	end
	super.onInit();
end
