    <LUACode>function update(bReadOnly)
	local sType=window.WeaponType.getValue()
	local bReadOnlyState;
	if sType=="melee" then
		bReadOnlyState=false;
	elseif sType=="beam" or sType=="spray" or sType=="micromissile/minigrenade" or sType=="seeker" then
		bReadOnlyState=true;
	elseif sType=="thrown" then
		bReadOnlyState=false;
	elseif sType=="system" then
		bReadOnlyState=true;
	else
		bReadOnlyState=false;
	end
				
	setReadOnly(bReadOnly or bReadOnlyState);
	local bVisible=not (bReadOnly and getValue()=="") and window.Type.isWeapon();
	setVisible(bVisible);
	window.LabelAttackSkill.setVisible(bVisible);
end

function UpdateAttackSkill()
	local sType=window.WeaponType.getValue();	
	if sType=="melee" then
		setValue("Melee");
		setReadOnly(false);
	elseif sType=="beam" or sType=="spray" or sType=="micromissile/minigrenade" or sType=="seeker" then
		setValue("Guns");
		setReadOnly(true);
	elseif sType=="thrown" then
		setValue("Athletics");
		setReadOnly(false);
	elseif sType=="system" then
		setValue("Interface");
		setReadOnly(true);
	else
		setValue("");
		setReadOnly(false);
	end
	UpdateToolTip();
end

function UpdateToolTip()
	local sType=window.WeaponType.getValue();	
	local sToolTip="";
	if sType=="melee" then
		sToolTip=Interface.getString("weapon_type_tooltip");
	elseif sType=="thrown" then
		sToolTip=Interface.getString("weapon_type_tooltip");
	end
	setTooltipText(sToolTip);
end

function onInit()
	UpdateToolTip()
end

-- 	Attack skills:
--		Melee and Improvised Weapons
--			Melee (*)
--			Exotic skill
--		Ranged weapons
--			Beam and Spray Weapons
--				Guns
--			Thrown Weapons
--				Athletics (*)
--				Exotic skill
--			Weapon Systems
--				Interface
--	(*) standard skill</LUACode>
