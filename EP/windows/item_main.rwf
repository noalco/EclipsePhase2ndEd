    <LUACode>function onInit()
	self.update();
end

function updateControl(sControl, bReadOnly)
	if not self[sControl] then
		return false;
	end
	
	return self[sControl].update(bReadOnly);
end

function update()
	local nodeRecord = getDatabaseNode();
	local bReadOnly = WindowManager.getReadOnlyState(nodeRecord);
	-- General section
	updateControl("Type", bReadOnly);
	updateControl("ServiceType", bReadOnly);
	updateControl("AugmentationType", bReadOnly);
	updateControl("Complexity", bReadOnly);
	updateControl("Restricted", bReadOnly);
	updateControl("TraitsNotes", bReadOnly);
	updateControl("LabelWare", bReadOnly);
	wareSubwindow.setVisible(not bReadOnly and Type.hasWare());
	updateControl("Description", bReadOnly);
	-- Weapon
	local bVisible=updateControl("DamageAverage", bReadOnly);
	bVisible=updateControl("DamageValue", bReadOnly) or bVisible;
	LabelDamage.setVisible(bVisible);
	updateControl("Specialization", bReadOnly);
	updateControl("LabelFM", bReadOnly);
	FiringModeSubwindow.setVisible(not bReadOnly and Type.isRangedWeapon());
	updateControl("Ammo", bReadOnly);
	updateControl("Range", bReadOnly);
	updateControl("AreaEffect", bReadOnly);
	updateControl("Radius", bReadOnly);
	updateControl("ArmorUsed", bReadOnly);
	updateControl("Quantity", bReadOnly);
	updateControl("DamageValueMod", bReadOnly);
	-- Armor
	bVisible=updateControl("EnergyAV", bReadOnly);
	bVisible=updateControl("KineticAV", bReadOnly) or bVisible;
	LabelArmorValue.setVisible(bVisible);
	updateControl("NoAdditionalArmorLayer", bReadOnly);
	-- Healing/Repair
	bVisible=updateControl("DamageHealingRate", bReadOnly);
	bVisible=updateControl("WoundHealingRate", bReadOnly) or bVisible;
	bVisible=updateControl("EffectDuration", bReadOnly) or bVisible;
	-- Divider: Se muestra si es un arma, una municin, una armadura o si se muestra algn control de sanacin
	Divider.update(Type.isWeapon() or Type.isKineticAmmo() or Type.isArmor() or Type.isArmorMod() or bVisible);
end
</LUACode>
