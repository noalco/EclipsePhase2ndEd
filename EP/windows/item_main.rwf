    <LUACode>function update(bReadOnly)
	setReadOnly(bReadOnly);
	local bVisible=not (bReadOnly and getValue()==0) and window.Type.isRangedWeapon();
	local bVisible2=not (bReadOnly and window.Ammo2.getValue()==0) and window.Type.isRangedWeapon();
	setVisible(bVisible);
	window.Ammo2.setVisible(bVisible2);
	window.LabelAmmo.setVisible(bVisible or bVisible2);
	window.LabelAmmo2.setVisible(bVisible and bVisible2);
	return bVisible;
end</LUACode>
