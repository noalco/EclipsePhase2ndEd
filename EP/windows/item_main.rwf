    <LUACode>function Update()
	local sAM="";
	local bAny=true;
	local sConnector="";
	if window.ApplicationMethodsSubwindow.subwindow.App.getValue()==1 then
		sAM="App";
		bAny=false;
	else
		if window.ApplicationMethodsSubwindow.subwindow.Dermal.getValue()==1 then
			sAM="D";
			sConnector=", ";
		else
			bAny=false;
		end
		if window.ApplicationMethodsSubwindow.subwindow.Inhalation.getValue()==1 then
			sAM=sAM..sConnector.."Inh";
			sConnector=", ";
		else
			bAny=false;
		end
		if window.ApplicationMethodsSubwindow.subwindow.Injected.getValue()==1 then
			sAM=sAM..sConnector.."Inj";
			sConnector=", ";
		else
			bAny=false;
		end
		if window.ApplicationMethodsSubwindow.subwindow.Oral.getValue()==1 then
			sAM=sAM..sConnector.."O";
		else
			bAny=false;
		end
	end
	if bAny then
		setValue("Any");
	else
		setValue(sAM);
	end
end</LUACode>
