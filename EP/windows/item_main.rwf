    <LUACode>function onInit()
	if getValue()=="" then
		setListValue("Miscelaneous");
	end
end

function onValueChanged()
	window.update();
	window.WeaponType.UpdateWeaponType();
	DB.setValue(window.getDatabaseNode(), "TypeIcon", "string", getValue());
end

function update(bReadOnly)
	setComboBoxReadOnly(bReadOnly);
end

function isWeapon()
	return isMeleeWeapon() or isRangedWeapon() or isMicromissilMinigrenade();
end

function isMeleeWeapon()
	return getValue()=="Melee Weapon";
end

function isRangedWeapon()
	return getValue()=="Ranged Weapon";
end

function isMicromissilMinigrenade()
	return getValue()=="Micromissile/Minigrenade";
end

function isGroupable()
	return isMicromissilMinigrenade() or isKineticAmmo();
end

function isKineticAmmo()
	return getValue()=="Kinetic Ammo";
end

function isArmor()
	return getValue()=="Armor";
end

function isWeaponAccesory()
	return getValue()=="Weapon Accesory";
end

function isArmorMod()
	return getValue()=="Armor Mod";
end

function isHealingRepair()
	return getValue()=="Healing/Repair";
end

function isAugmentation()
	return getValue()=="Augmentation";
end

function isService()
	return getValue()=="Service";
end

function isMiscelaneous()
	return getValue()=="Miscelaneous";
end

function isAPP_ALI()
	return getValue()=="APPs &amp; ALIs";
end

function isDrugToxin()
	return getValue()=="Drugs &amp; Toxins";
end

function hasWare()
	return isMeleeWeapon() or isRangedWeapon() or isArmor() or isAugmentation();
end

function hasTraitsNotes()
	return isWeapon() or isKineticAmmo() or isArmor() or isArmorMod();
end

function hasSubtype()
	return isService() or isMiscelaneous() or isAPP_ALI() or isAugmentation() or isDrugToxin();
end

function getSubtype()
	if isService() then
		return "Service Group";
	elseif isMiscelaneous() then
		return "Misc.  Group";
	elseif isAPP_ALI() then
		return "APP/ALI Group";
	elseif isAugmentation() then
		return "Augm. Group";
	elseif isDrugToxin() then
		return "D/T Group";
	else
		return "";
	end
end
