    <LUACode>function onInit()
	self.update();
end

function updateControl(sControl, bReadOnly)
	if not self[sControl] then
		return false;
	end
	
	return self[sControl].update(bReadOnly);
end

function update()
	local nodeRecord = getDatabaseNode();
	local bReadOnly = WindowManager.getReadOnlyState(nodeRecord);
	for _,c in pairs(getControls()) do
		if c.update then
			c.update(bReadOnly)
		end
	end
	Divider.update();
end

function UpdateStringAndLabel(Control, bReadOnly, bOnItem)
	local bVisible=false;
	if Control then
		Control.setReadOnly(bReadOnly);
		bVisible=not (bReadOnly and Control.getValue()=="") and bOnItem;
		Control.setVisible(bVisible);
		if self["Label"..Control.getName()] then
			self["Label"..Control.getName()].setVisible(bVisible);
		end
	end
	return bVisible;
end

function UpdateComboBoxAndLabel(Control, bReadOnly, bOnItem)
	local bVisible=false;
	if Control then
		Control.setComboBoxReadOnly(bReadOnly);
		bVisible=not (bReadOnly and Control.getValue()=="") and bOnItem;
		Control.setComboBoxVisible(bVisible)
		if self["Label"..Control.getName()] then
			self["Label"..Control.getName()].setVisible(bVisible);
		end	end
	return bVisible;
end

function UpdateNumberAndLabel(Control, bReadOnly, bOnItem)
	local bVisible=false;
	if Control then
		Control.setReadOnly(bReadOnly);
		bVisible=not (bReadOnly and Control.getValue()==0) and bOnItem;
		Control.setVisible(bVisible);
		if self["Label"..Control.getName()] then
			self["Label"..Control.getName()].setVisible(bVisible);
		end
	end
	return bVisible;
end

