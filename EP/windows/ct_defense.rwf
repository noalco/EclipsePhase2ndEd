    <LUACode>function onInit()
	if parentcontrol.window.isPC() then
		local nodeChar = parentcontrol.window.link.getTargetDatabaseNode();
		EnergyAV.setLink(nodeChar.createChild("Armor.EnergyAV", "number"), true);
		KineticAV.setLink(nodeChar.createChild("Armor.KineticAV", "number"), true);
		
		Melee.setLink(nodeChar.createChild("Combat.Melee", "number"), true);
--		MeleeSpecialization.setLink(nodeChar.createChild("Combat.MeleeSpecialization", "number"), true);
		HasMeleeSpecialization.setLink(nodeChar.createChild("Combat.HasMeleeSpecialization", "number"), true);
		MeleeSpecializationName.setLink(nodeChar.createChild("Combat.MeleeSpecializationName", "string"), true);
		
		Fray.setLink(nodeChar.createChild("Combat.Fray", "number"), true);
--		Fray2.setLink(nodeChar.createChild("Combat.Fray2", "number"), true);
--		FraySpecialization.setLink(nodeChar.createChild("Combat.FraySpecialization", "number"), true);
--		Fray2Specialization.setLink(nodeChar.createChild("Combat.Fray2Specialization", "number"), true);
		HasFraySpecialization.setLink(nodeChar.createChild("Combat.HasFraySpecialization", "number"), true);
		FraySpecializationName.setLink(nodeChar.createChild("Combat.FraySpecializationName", "string"), true);
		
		Initiative.setLink(nodeChar.createChild("Initiative", "number"), true);
	end
end
